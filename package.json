    "crypto-browserify": "^3.12.0",
    "d3": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "is-electron": "^2.2.0",
    "minimist": "^1.2.5",
    "mongodb": "^3.6.9",
    "mongoose": "^5.13.0",
    "node-sass": "^6.0.1",
    "perf_hooks": "^0.0.1",
    "pg": "^8.6.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "recharts": "^2.0.9",
    "sass": "^1.35.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start:electron": "ELECTRON_START_URL=http://localhost:3000/ electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\" \"sass --watch src/stylesheets:src/stylesheets\"",
    "dev-server": "cross-env NODE_ENV=development webpack serve --config ./webpack.development.js > dev-scripts/webpack-dev-server.log 2> dev-scripts/webpack-dev-server-error.log",
    "devWebpack": "concurrently --success first \"node dev-scripts/prepareDevServer.js\" \"node dev-scripts/launchDevServer.js\" -k",
    "electron": "wait-on tcp:3000 && electron .",
    "server": "node server/server.js",
    "start2": "concurrently \"npm run start\" \"npm run server\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.3",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "electron": "^13.1.5",
    "electron-is-dev": "^2.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "wait-on": "^5.1.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  }
}
